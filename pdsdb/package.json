{
  "name": "frontend",
  "version": "1.0.0",
  "author": "Timothy Catana",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@electron/remote": "^2.0.9",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.1.3",
    "electron-is-dev": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-router-dom": "^6.4.2",
    "react-scripts": "5.0.1",
    "styled-components": "^5.3.6",
    "web-vitals": "^2.1.4"
  },
  "main": "public/main.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",

    "server:start": "node ./server/index.js",
    "server:devStart": "nodemon ./server/index.js",

    "electron:serve": "concurrently -k \"cross-env BROWSER=none yarn start\" \"yarn electron:start\"",
    "electron:start": "wait-on http://127.0.0.1:3000 && electron .",
    "electron:build": "yarn build && electron-builder -c.extraMetadata.main=build/main.js",

    "app:devStart": "concurrently  \"yarn run start\" \"yarn run server:start\"",
    "app:electronStart": "concurrently  \"yarn run server:start\" \"yarn run electron:serve\"",
 
    "dist": "electron-builder"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "electron": "^22.0.0",
    "electron-builder": "^23.6.0",
    "prettier": "^2.8.1",
    "wait-on": "^6.0.1"
  },
  "homepage": "./"
}